{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/css/(.*)", 
      "destination": "/css/$1"
    },
    {
      "source": "/js/(.*)", 
      "destination": "/js/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
